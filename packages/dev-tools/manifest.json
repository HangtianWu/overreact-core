{
  "name": "overreact DevTools",
  "description": "DevTools for overreact developers",
  "version": "0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "src/extension/background.js"
  },
  "host_permissions": [
    "file:///*",
    "http://*/*",
    "https://*/*"
  ],
  "devtools_page": "pages/devtools.html",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "inject_global_hook.js"
      ],
      "run_at": "document_start"
    }
  ]
}